# PropRadar Environment Configuration

# Google Maps API Keys
GOOGLE_MAPS_ANDROID_API_KEY=your_android_api_key_here
GOOGLE_MAPS_IOS_API_KEY=your_ios_api_key_here

# Property Data APIs (Optional)
# HDB resale data doesn't require an API key
URA_API_KEY=your_ura_api_key_here  # Optional - for private property data
ONEMAP_API_KEY=your_onemap_api_key_here  # Optional - for geocoding

# API Configuration
API_BASE_URL=https://api.propradar.sg
API_REQUEST_TIMEOUT_MS=5000
API_MAX_RETRIES=3

# Data Refresh Settings
HDB_DATA_REFRESH_INTERVAL_DAYS=7  # How often to fetch fresh HDB data

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true

# Backend Environment (development, staging, production)
ENVIRONMENT=production